<Window x:Class="GazeTrackerUI.Mappings.MappingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MappingsWindow" Height="200" Width="375" Closing="Window_Closing" ContentRendered="Window_ContentRendered">
    <Grid>
        <Label Content="Application" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="81,12,0,0" Name="appComboBox" VerticalAlignment="Top" Width="260" DropDownClosed="appComboBox_DropDownClosed" DropDownOpened="appComboBox_DropDownOpened" SelectionChanged="appComboBox_SelectionChanged">
            <ComboBoxItem />
        </ComboBox>
        <Button Content="Cancel" Height="23" HorizontalAlignment="Right" Margin="0,126,12,0" Name="cancelButton" VerticalAlignment="Top" Width="75" Click="cancelButton_Click" />
        <Button Content="Save" Height="23" HorizontalAlignment="Left" Margin="185,126,0,0" Name="saveButton" VerticalAlignment="Top" Width="75" Click="saveButton_Click" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="81,56,0,0" Name="leftTextBox" VerticalAlignment="Top" Width="100" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="81,88,0,0" Name="rightTextBox" VerticalAlignment="Top" Width="100" />
        <Label Content="Left" Height="28" HorizontalAlignment="Left" Margin="47,54,0,0" Name="label2" VerticalAlignment="Top" />
        <Label Content="Right" Height="28" HorizontalAlignment="Left" Margin="40,0,0,47" Name="label3" VerticalAlignment="Bottom" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="241,56,0,0" Name="upTextBox" VerticalAlignment="Top" Width="100" />
        <Label Content="Up" Height="28" HorizontalAlignment="Left" Margin="207,54,0,0" Name="label4" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="241,88,0,0" Name="downTextBox" VerticalAlignment="Top" Width="100" />
        <Label Content="Down" Height="28" HorizontalAlignment="Left" Margin="192,86,0,0" Name="label5" VerticalAlignment="Top" />
    </Grid>
</Window>
